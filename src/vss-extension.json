{
  "manifestVersion": 1,
  "id": "DynamicLoadingData",
  "name": "Dynamic Loading Data",
  "version": "1.0.7",
  "publisher": "VladyslavHorbachov",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "This task print out the content of the given directory",
  "public": true,
  "categories": [
    "Azure Pipelines"
  ],
  "content": {
    "details": {
      "path": "Marketplace.md"
    }
  },
  "icons": {
    "default": "Images/icon.png"
  },
  "files": [
    {
      "path": "Tasks/TaskWithDynamicallyLoadedData"
    },
    {
      "path": "Tasks/TestCreateAndQueryChangeRequest"
    }
  ],
  "contributions": [
    {
      "id": "TaskWithDynamicallyLoadedData",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/TaskWithDynamicallyLoadedData"
      }
    },
    {
      "id": "test-serviceNowchangerequests-query-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/TestCreateAndQueryChangeRequest"
      }
    }
  ]
}
